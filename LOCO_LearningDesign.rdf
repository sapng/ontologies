    <!-- http://www.lornet.org/LOCO#asCompetency -->

    <owl:ObjectProperty rdf:about="http://www.lornet.org/LOCO#asCompetency">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#LearningObjective"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#Prerequisite"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Learning objective or prerequisites expressed in terms of competencies:1) target competencies in the case of learning objectives; 2) entry competencies in the case of prerequisites.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.lornet.org/LOCO#hasAct -->

    <owl:ObjectProperty rdf:about="http://www.lornet.org/LOCO#hasAct">
        <rdfs:domain rdf:resource="http://www.lornet.org/LOCO#Play"/>
        <rdfs:range rdf:resource="http://www.lornet.org/LOCO#Act"/>
    </owl:ObjectProperty>
    


    <!-- http://www.lornet.org/LOCO#hasActivity -->

