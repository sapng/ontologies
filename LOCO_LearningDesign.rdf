    <!-- http://www.lornet.org/LOCO#title -->

    <owl:DatatypeProperty rdf:about="http://www.lornet.org/LOCO#title">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#Act"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#Component"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#LearningDesign"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#Play"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#ResourceDescription"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#RolePart"/>
                    <rdf:Description rdf:about="http://www.lornet.org/LOCO#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


